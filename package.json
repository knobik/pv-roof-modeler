{
  "name": "pv-roof-modeler",
  "version": "1.0.0",
  "description": "React component library for annotating aerial images with polygon outlines in a 3D canvas",
  "type": "module",
  "main": "./dist/pv-roof-modeler.umd.cjs",
  "module": "./dist/pv-roof-modeler.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/pv-roof-modeler.js",
      "require": "./dist/pv-roof-modeler.umd.cjs"
    },
    "./styles.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:demo": "vite build --config vite.config.demo.ts",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/suncalc": "^1.9.2",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.2.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "react",
    "threejs",
    "3d",
    "polygon",
    "annotation",
    "roof",
    "solar",
    "pv",
    "aerial-imagery"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "suncalc": "^1.9.0",
    "three": "^0.182.0"
  }
}
